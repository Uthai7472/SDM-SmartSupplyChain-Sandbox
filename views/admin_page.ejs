<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>ADMIN</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css">
</head>
<body>
    <%- include('sub_views/header') %>

    <table class="table">
        <tr>
            <th>ID</th>
            <th>Username</th>
            <th>Email</th>
            <th>Level</th>
            <th>Actions</th>
        </tr>
        
            <% user_datas.forEach((user_data) => { %>
                <tr>
                    <td><%= user_data.id %></td>
                    <td><%= user_data.username %></td>
                    <td><%= user_data.email %></td>
                    <td><%= user_data.level %></td>
                    <td>
                        <!-- <a class="btn btn-warning" href="/user/<%= user_data.id %>/edit?name=<%= user_data.username %>&email=<%= user_data.email %>">Edit</a> -->
                        <a class="btn btn-warning" href="/admin_page/edit?id=<%= user_data.id %>&name=<%= user_data.username %>&email=<%= user_data.email %>&level=<%= user_data.level %>>Edit</a>
                        <a class="btn btn-danger" href="/delete_user">Delete</a>
                    </td>
                </tr>
            <% }) %>
    </table>
    
</body>
</html>