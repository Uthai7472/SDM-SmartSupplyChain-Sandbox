<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css">
</head>
<body>

    <%- include('sub_views/header') %>

    <div>
        <table class="table">
            <thead>
                <tr>
                    <th>NO.</th>
                    <th>QR PACKING LIST</th>
                </tr>
            </thead>
            <tbody>
                <% let count = 0; %>
                <% if (qr_packingList && qr_packingList.length > 0) { %>
                    <% qr_packingList.forEach((row) => { %>
                    <% count++; %>
                    <tr>
                        <td><%= count %></td>
                        <td><%= row.qr_packingList %></td>
                    </tr>
                    <% }) %>
                <% } else { %>
                    <tr>
                    <td colspan="2">No QR packing list available</td>
                    </tr>
                <% } %>
            </tbody>
        </table>
    </div>
    
</body>
</html>