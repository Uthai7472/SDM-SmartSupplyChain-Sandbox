<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css">
    <style>
        /* h1 {
            color: rgb(143, 229, 158);
        } */
    </style>
</head>
<body>
    <%- include('sub_views/header') %>

    <div>
        <h1>Scan QR Master Packing List</h1>
    </div>

    <div>
        <form action="/admin_page/receive_scan_master/master_receiving" method="POST">
            <label for="qr_packingList" class="form-control" name="qr_packingList">QR Packing List</label>
            <input type="text" id="qr_packingList" class="qr_packingList form-control mb-3" name="qr_packingList">
            <button type="submit" class="btn btn-primary">Submit</button>
        </form>
    </div>


    <script>
        window.onload = function() {
            document.getElementById("qr_packingList").focus();
        };

        <% if (errorMessage.length > 0) { %>
            alert('<%= errorMessage[0] %>');
        <% } %>
        // window.location.href = '/admin_page/receive_scan_master';
    </script>
    
</body>
</html>